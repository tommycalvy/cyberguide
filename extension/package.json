{
  "name": "cyberguide",
  "version": "0.0.1",
  "description": "Your Cyberspace Tourguide!",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:rspack": "rspack build",
    "dev:css": "tailwindcss -o dist/main.css --watch",
    "build": "npm-run-all build:css build:rspack",
    "build:rspack": "rspack build",
    "build:css": "tailwindcss -o dist/main.css --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tommycalvy/cyberguide.git#master"
  },
  "author": "Tommy Calvy",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tommycalvy/cyberguide/issues"
  },
  "homepage": "https://github.com/tommycalvy/cyberguide/tree/master#readme",
  "devDependencies": {
    "@rspack/cli": "^0.4.5",
    "@rspack/core": "^0.4.5",
    "autoprefixer": "^10.4.16",
    "glob": "^10.3.10",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.4.33",
    "postcss-loader": "^7.3.4",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "webextension-polyfill": "^0.10.0"
  }
}
